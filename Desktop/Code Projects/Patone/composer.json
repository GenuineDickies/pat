{
    "name": "genuinedickies/patone",
    "description": "Roadside Assistance Admin Platform",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Patone\\": "backend/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-unit": "phpunit --testsuite Unit",
        "test-integration": "phpunit --testsuite Integration",
        "test-security": "phpunit --testsuite Security"
    }
}
